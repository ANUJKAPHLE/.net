@model List<FacultyModel>  

<h1>Faculties</h1>

<table class=" table table-striped">
     <thead>
         <tr>
             
           <th>Name</th>
           <th>Seats</th>
           <th>Fee</th>

         </tr>
     </thead>
     <tbody>
         @foreach(FacultyModel f in Model)
         {
         <tr>
             <td>@f.Name</td>
             <td>@f.Seats</td>
             <td>@f.TotalFee</td>
         </tr>
         }
     </tbody>
</table>